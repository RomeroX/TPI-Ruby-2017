<a id="title">Ingresar</a>

<%= simple_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <%= f.input :email %><br />
    <%= f.input :password %><br />
     

  <% if devise_mapping.rememberable? -%>
     <%= f.check_box :remember_me %>
      <%= f.label :remember_me %> 
  <% end -%>

 <%= f.submit "Entrar" %>
<% end %>

